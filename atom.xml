---
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Qndev's blog</title>
 <link href="https://qndev.github.io/blog/atom.xml" rel="self"/>
 <link href="https://qndev.github.io/blog"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>https://qndev.github.io/blog</id>
 <author>
   <name>Nguyen Dinh Quang</name>
   <email>quangnd.hust@gmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="https://qndev.github.io/blog{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>https://qndev.github.io/blog{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
